package src;

import java.awt.Color;

public class ColorTool extends Tool {
	
	String name;
	private Color color;
	Canvas can;

	public ColorTool(String name, Color c, Canvas can) {
		this.name = name;
		this.color = c;
		this.can = can;	
	}

	@Override
	public void execute() {
		can.setColor(color);
	}

}
